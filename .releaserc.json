{
  "branches": ["master"],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          { "type": "refactor", "release": "patch" },
          { "type": "chore", "release": "patch" }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "./CHANGELOG.md"
      }
    ],
    "@semantic-release/git",
    "@semantic-release/github",
    [
      "@semantic-release/github",
      {
        "assets": [
          { "path": "./multi_waka_ubuntu-latest", "label": "multiwaka_linux_x64" },
          { "path": "./multi_waka_windows-latest", "label": "multiwaka_windows_x64.exe" },
          { "path": "./multi_waka_macos-latest", "label": "multiwaka_macos_arm64" }
        ]
      }
    ],
    [
      "semantic-release-cargo",
      {
        "allFeatures": true,
        "check": true,
        "checkArgs": [],
        "publish": false,
        "alwaysVerifyToken": false
      }
    ]
  ],
  "dryRun": false
}